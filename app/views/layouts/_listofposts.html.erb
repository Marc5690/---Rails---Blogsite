<div class="center">
  <table style="width:100%">
    <tr>
    <th>Date</th>
    <th>Title</th>
    <th align="right"><button onClick="changePostsVisibility()">Tutorials</button></th>
  </tr>
    
    <% @posts&.each do |post| %>
      <tr>
      <td><%= post.daterelease.strftime("%Y/%m")%></td>  
      <td colspan="2"><%= link_to post.title, post%></td></tr>
    <% end %>
  
  </table>
</div>