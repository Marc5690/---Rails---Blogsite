<div class="center">
  <table style="width:100%">
    <tr>
    <th>Date</th>
    <th>Title</th>
    <th align="right"><button onClick="changePostsVisibility()" , >Everything</button></th>
  </tr>
    
    <% @tutorials&.each do |tutorial| %>
      <tr>
      <td><%= tutorial.daterelease.strftime("%Y/%m")%></td>  
      <td colspan="2"><%= link_to tutorial.title, tutorial%></td></tr>
    <% end %>
  
  </table>
</div>